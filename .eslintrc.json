{
  "extends": [
    "google",
    "react-app",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "require-jsdoc": "off",
    "no-invalid-this": "off",
    "max-len": ["error", {
      "code": 120,
      "tabWidth": 2
    }],
    "indent": ["error", 2],
    "object-curly-spacing": ["error","always"]
  }
}
